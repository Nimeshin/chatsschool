{
    "name": "sathyasai/school-website",
    "description": "Sathya Sai School Chatsworth Website",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "phpmailer/phpmailer": "^6.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SathyaSaiSchool\\": "includes/classes/"
        }
    },
